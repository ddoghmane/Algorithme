
public class Point {
	public static void main(String[] args) {
		Point a = new Point('G', 345);
	}

public Point(char c ,double x) {
	nom = c;
	abscisses=x;}
			public void affiche ()
	{ 
				System.out.println("Point de nom " + nom + " d'abscisse " + abscisses) ; }
	




	private char nom;
	private double abscisses;
}
